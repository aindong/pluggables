{
  "name": "aindong/pluggables",
  "description": "Pluggable features/Modular Structure helper for laravel 5",
  "keywords": ["modules", "laravel", "aindong", "pluggables"],
  "license": "MIT",
  "authors": [
    {
      "name": "Alleo Indong",
      "email": "aindong@koodi.ph"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/database": "~5.0",
    "illuminate/support": "~5.0",
    "illuminate/filesystem": "~5.0",
    "illuminate/config": "~5.0",
    "illuminate/routing": "~5.0",
    "illuminate/console": "~5.0"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Aindong\\Pluggables\\": "src/Aindong/Pluggables/"
    }
  },
  "minimum-stability": "dev",
  "suggest": {
    "wikimedia/composer-merge-plugin": "Allows the ability to create and merge composer.json files for your individual modules for module-specific dependency management."
  }
}